{
  "name": "@mkabanau/components",
  "version": "0.0.1",
  "description": "components and functions for a wallet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "build": "npx tsc"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/.history/"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "preset": "ts-jest/presets/default-esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkabanau/ucan-plugin.git"
  },
  "keywords": [
    "ci",
    "cd",
    "ts",
    "registry"
  ],
  "author": "maksim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mkabanau/ucan-plugin/issues"
  },
  "homepage": "https://github.com/mkabanau/ucan-plugin#readme",
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "publishConfig": {
    "@mkabanau:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@peculiar/webcrypto": "1.1.4",
    "@transmute/jose-ld": "^0.7.0-unstable.80",
    "@transmute/x25519-key-pair": "^0.7.0-unstable.80",
    "@types/node": "^18.15.11"
  }
}
